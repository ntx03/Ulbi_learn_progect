{
    "name": "ulbi_progect",
    "version": "0.0.1",
    "description": "learning progect ulbi",
    "main": "index.js",
    "scripts": {
        "dev": "webpack serve --env port=3998",
        "dev:vite": "vite",
        "prod:vite": "vite build",
        "dev:start": "concurrently \" npm run server\" \" npm run dev\"",
        "dev:start:vite": "concurrently \" npm run server\" \" npm run dev:vite\"",
        "build:prod": "webpack  --env mode=production apiUrl=https://production-progect-server.vercel.app/",
        "build:dev": "webpack --env mode=development",
        "lint:ts": "eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
        "fix:all": "npm run lint:ts:fix && npm run lint:scss:fix",
        "prettier": "npx prettier --write **/*.{ts,tsx,json}",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:e2e": "cypress open",
        "storybook": "storybook dev -p 6006 -c ./config/storybook",
        "storybook:build": "storybook build -c ./config/storybook",
        "test:ui": "npx loki test",
        "test:ui:ok": "npx loki approve",
        "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "visual:report": "npm run visual:report:json && npm run visual:report:html",
        "visual:report:json": "node scripts/generate-visual-json-report.js",
        "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "server": "node ./json-server/index.js",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "remove-feature-by-flag": "ts-node ./scripts/remove-feature.ts"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": "eslint",
        "**/*.scss": "stylelint"
    },
    "author": "kachur A.L",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/plugin-transform-runtime": "^7.22.10",
        "@babel/plugin-transform-typescript": "^7.22.10",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
